{
  "name": "@ijia/data",
  "version": "0.0.1",
  "fmt": {
    "lineWidth": 120
  },
  "tasks": {
    "check-type": "deno check src",
    "doc:gen": "deno doc --name @ijia/data--html --output=docs/api src/db.ts src/oos.ts src/query.ts src/testlib.ts src/util.ts src/yoursql.ts",
    "build": "deno run -A build/build.ts"
  },
  "lint": {
  },
  "nodeModulesDir": "manual",
  "imports": {
    "@ijia/data/": "./src/"
  },
  "exports": {
    "./db": "./src/db.ts",
    "./oos": "./src/oos.ts",
    "./query": "./src/query.ts",
    "./testlib": "./src/testlib.ts",
    "./yoursql": "./src/yoursql.ts"
  }
}
